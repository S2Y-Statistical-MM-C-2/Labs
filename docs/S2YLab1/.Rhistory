annotate("text", x = 1.55, y = 0.05, label = TeX("\\p-value$/2$"), size = 2.5, colour="blue") +
annotate("text", x = 1.4, y = -0.04, label = "the value of the observed Standardised Test Statistic", vjust = 0.4, size=2.5, colour="blue") +
