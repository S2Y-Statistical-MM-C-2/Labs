mu <- seq(from = 1.8, to = 2.2, length = 500)
n <- 150
alpha <- 0.05
sigma <- 0.5
lcv <- qnorm(alpha/2, 2, sigma/sqrt(n))
ucv <- qnorm(1 - alpha/2, 2, sigma/sqrt(n))
Power <- pnorm(lcv, mu, sigma/sqrt(n)) + pnorm(ucv, mu, sigma/sqrt(n), lower = FALSE)
DF <- data.frame(mu, Power)
